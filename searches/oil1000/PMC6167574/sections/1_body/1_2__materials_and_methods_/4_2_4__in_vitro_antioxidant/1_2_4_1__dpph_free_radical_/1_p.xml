            <mi mathvariant="normal" class="mi">A</mi>
           </mrow>
           <mrow class="mrow">
            <mi mathvariant="normal" class="mi">c</mi>
           </mrow>
          </msub>
         </mrow>
        </mfenced>
       </mrow>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">A</mi>
         </mrow>
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">m</mi>
          <mi mathvariant="normal" class="mi">a</mi>
          <mi mathvariant="normal" class="mi">x</mi>
         </mrow>
        </msub>
       </mrow>
      </mfrac>
      <mo class="mo">×</mo>
      <mn mathvariant="normal" class="mn">100</mn>
      <mi class="mi">%</mi>
