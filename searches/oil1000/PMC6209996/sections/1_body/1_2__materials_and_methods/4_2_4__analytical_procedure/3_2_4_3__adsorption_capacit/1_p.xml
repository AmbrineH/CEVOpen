        </msub>
       </mrow>
       <mi class="mi">M</mi>
      </mfrac>
      <mo class="mo">×</mo>
      <mi class="mi">v</mi>
      <mo class="mo">,</mo>
     </mrow>
     <mrow class="mrow">
      <mtext class="mtext"> </mtext>
      <mrow class="mrow">
       <mo class="mo">(</mo>
       <mrow class="mrow">
