      <mi class="mi">u</mi>
      <mi class="mi">m</mi>
      <mtext class="mtext"> </mtext>
      <mi class="mi">c</mi>
      <mi class="mi">a</mi>
      <mi class="mi">p</mi>
      <mi class="mi">a</mi>
      <mi class="mi">c</mi>
      <mi class="mi">i</mi>
      <mi class="mi">t</mi>
      <mi class="mi">y</mi>
      <mtext class="mtext"> </mtext>
      <mrow class="mrow">
       <mo class="mo">(</mo>
       <mrow class="mrow">
        <msub class="msub">
         <mi class="mi">q</mi>
         <mi class="mi">e</mi>
        </msub>
       </mrow>
       <mo class="mo">)</mo>
      </mrow>
      <mo class="mo">=</mo>
      <mfrac class="mfrac">
       <mrow class="mrow">
        <msub class="msub">
         <mi class="mi">C</mi>
         <mi class="mi">i</mi>
        </msub>
        <mo class="mo">−</mo>
        <msub class="msub">
         <mi class="mi">C</mi>
