      <mi class="mi">p</mi>
      <mi class="mi">t</mi>
      <mi class="mi">i</mi>
      <mi class="mi">o</mi>
      <mi class="mi">n</mi>
      <mtext class="mtext"> </mtext>
      <mi class="mi">e</mi>
      <mi class="mi">q</mi>
      <mi class="mi">u</mi>
      <mi class="mi">i</mi>
      <mi class="mi">l</mi>
      <mi class="mi">i</mi>
      <mi class="mi">b</mi>
