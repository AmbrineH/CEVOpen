     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">S</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">ads</mi>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">H</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">2</mn>
      <mrow class="mrow">
       <mo class="mo">(</mo>
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">g</mi>
       </mrow>
       <mo class="mo">)</mo>
      </mrow>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <msub class="msub">
