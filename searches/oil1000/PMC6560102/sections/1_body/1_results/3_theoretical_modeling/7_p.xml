       <mo class="mo">)</mo>
      </mrow>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">CH</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">3</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">SH</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">ads</mi>
     </mrow>
    </msub>
   </math>
   <graphic xlink:href="41467_2019_10526_Article_Equ2.gif" position="anchor" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
  </alternatives>
 </disp-formula>
 <disp-formula id="Equ3" class="disp-formula">
  <label class="label">3</label>
  <alternatives class="alternatives">
   <tex-math id="M5" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\frac{{[{\mathrm{S}}_{{\mathrm{ads}}}]}}{{[{\mathrm{CH}}_3{\mathrm{SH}}_{{\mathrm{ads}}}]}} = \frac{{k_1 + k_{ - 2}{\mathrm{P}}_{{\mathrm{H}}_2{\mathrm{S}}}}}{{k_2{\mathrm{P}}_{{\mathrm{H}}_2}{\mathrm{P}}_{{\mathrm{CH}}_3{\mathrm{SH}}}}}$$\end{document}</tex-math>
   <math id="M6" class="math">
    <mfrac class="mfrac">
     <mrow class="mrow">
      <mrow class="mrow">
       <mo class="mo">[</mo>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">S</mi>
         </mrow>
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">ads</mi>
         </mrow>
        </msub>
       </mrow>
       <mo class="mo">]</mo>
      </mrow>
     </mrow>
     <mrow class="mrow">
      <mrow class="mrow">
       <mo class="mo">[</mo>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">CH</mi>
         </mrow>
         <mrow class="mrow">
          <mn class="mn">3</mn>
         </mrow>
        </msub>
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">SH</mi>
         </mrow>
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">ads</mi>
         </mrow>
        </msub>
       </mrow>
       <mo class="mo">]</mo>
      </mrow>
     </mrow>
    </mfrac>
    <mo class="mo">=</mo>
    <mfrac class="mfrac">
     <mrow class="mrow">
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">k</mi>
       </mrow>
       <mrow class="mrow">
        <mn class="mn">1</mn>
       </mrow>
      </msub>
      <mo class="mo">+</mo>
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">k</mi>
       </mrow>
       <mrow class="mrow">
        <mo class="mo">-</mo>
        <mn class="mn">2</mn>
       </mrow>
      </msub>
      <msub class="msub">
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">P</mi>
       </mrow>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">H</mi>
         </mrow>
         <mrow class="mrow">
          <mn class="mn">2</mn>
         </mrow>
        </msub>
        <mi mathvariant="normal" class="mi">S</mi>
       </mrow>
      </msub>
     </mrow>
     <mrow class="mrow">
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">k</mi>
       </mrow>
       <mrow class="mrow">
        <mn class="mn">2</mn>
       </mrow>
      </msub>
      <msub class="msub">
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">P</mi>
       </mrow>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">H</mi>
         </mrow>
         <mrow class="mrow">
          <mn class="mn">2</mn>
         </mrow>
        </msub>
       </mrow>
      </msub>
      <msub class="msub">
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">P</mi>
       </mrow>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">CH</mi>
         </mrow>
         <mrow class="mrow">
          <mn class="mn">3</mn>
         </mrow>
        </msub>
        <mi mathvariant="normal" class="mi">SH</mi>
       </mrow>
      </msub>
     </mrow>
    </mfrac>
   </math>
   <graphic xlink:href="41467_2019_10526_Article_Equ3.gif" position="anchor" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
