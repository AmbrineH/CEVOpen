    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">SH</mi>
     </mrow>
     <mrow class="mrow">
      <mrow class="mrow">
       <mo class="mo">(</mo>
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">g</mi>
       </mrow>
       <mo class="mo">)</mo>
      </mrow>
     </mrow>
    </msub>
    <mi class="mi">⇌</mi>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">H</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">2</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">S</mi>
     </mrow>
     <mrow class="mrow">
      <mrow class="mrow">
       <mo class="mo">(</mo>
