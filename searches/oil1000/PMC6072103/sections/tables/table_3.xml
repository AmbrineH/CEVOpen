<?xml version="1.0" encoding="UTF-8"?>
<table-wrap id="pone.0201835.t003" orientation="portrait" position="float" class="table-wrap">
 <object-id pub-id-type="doi" class="object-id">10.1371/journal.pone.0201835.t003</object-id>
 <label class="label">Table 3</label>
 <caption class="caption">
  <title class="title">Parameters used in GC/MS analysis of essential oils.</title>
 </caption>
 <alternatives class="alternatives">
  <graphic id="pone.0201835.t003g" xlink:href="pone.0201835.t003" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
  <table frame="hsides" rules="groups" class="table">
   <colgroup span="1" class="colgroup">
    <col align="left" valign="middle" span="1" class="col"/>
    <col align="left" valign="middle" span="1" class="col"/>
   </colgroup>
   <thead class="thead">
    <tr class="tr">
     <th align="justify" rowspan="1" colspan="1" class="th">Column</th>
     <th align="justify" rowspan="1" colspan="1" class="th">Zebron ZB5-MS</th>
    </tr>
   </thead>
   <tbody class="tbody">
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Stationary phase</td>
     <td align="left" rowspan="1" colspan="1" class="td">5%phenyl-95%-Dimethyl polysiloxane copolymer</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Dimensions</td>
     <td align="justify" rowspan="1" colspan="1" class="td">30 m long 0.25 mm × 0.25 μm</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Sample quantity injected</td>
     <td align="justify" rowspan="1" colspan="1" class="td">0.5 μl</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Carrier gas</td>
     <td align="justify" rowspan="1" colspan="1" class="td">Helium at a constant flow of 1 ml min
      <sup class="sup">-1</sup>
     </td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Oven profile</td>
     <td align="justify" rowspan="1" colspan="1" class="td">50°C to 275°C at 5°C min
      <sup class="sup">-1</sup>. Final temperature held for 5 min
     </td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Inlet temperature</td>
     <td align="justify" rowspan="1" colspan="1" class="td">250°C</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Split ratio</td>
     <td align="justify" rowspan="1" colspan="1" class="td">100:1</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Ionization mode</td>
     <td align="justify" rowspan="1" colspan="1" class="td">Electron impact positive mode</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Ion source temperature</td>
     <td align="justify" rowspan="1" colspan="1" class="td">250°C</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Multiplier</td>
     <td align="justify" rowspan="1" colspan="1" class="td">149 V</td>
    </tr>
    <tr class="tr">
     <td align="justify" rowspan="1" colspan="1" class="td">Mass range analysed</td>
     <td align="justify" rowspan="1" colspan="1" class="td">1–300</td>
    </tr>
   </tbody>
  </table>
 </alternatives>
</table-wrap>
