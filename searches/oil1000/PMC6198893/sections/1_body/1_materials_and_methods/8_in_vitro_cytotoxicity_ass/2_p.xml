    <mo stretchy="false" class="mo">(</mo>
    <mi class="mi">%</mi>
    <mo stretchy="false" class="mo">)</mo>
    <mo class="mo">=</mo>
    <mfrac class="mfrac">
     <mrow class="mrow">
      <mtext class="mtext">Absorbance</mtext>
      <mi mathvariant="bold" class="mi"> </mi>
      <mtext class="mtext">sample</mtext>
      <mi mathvariant="bold" class="mi"> </mi>
      <mo stretchy="false" class="mo">(</mo>
      <mtext class="mtext">mean</mtext>
      <mo stretchy="false" class="mo">)</mo>
     </mrow>
     <mrow class="mrow">
      <mtext class="mtext">Absorbance control</mtext>
      <mspace width="0.2em" class="mspace"/>
      <mo stretchy="false" class="mo">(</mo>
      <mtext class="mtext">mean</mtext>
      <mo stretchy="false" class="mo">)</mo>
     </mrow>
    </mfrac>
    <mo class="mo">×</mo>
    <mn class="mn">100</mn>
    <mi class="mi">%</mi>
   </mrow>
  </math>
  <label class="label">(5)</label>
 </disp-formula>
</p>
