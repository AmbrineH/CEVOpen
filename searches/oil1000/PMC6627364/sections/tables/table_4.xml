   <tr class="tr">
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">Acid hydrolysis</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">Carrageenan (C)</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">Mw: κ-, 510–4000; ι-, 110–3300; λ-, 660–5800</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">[
     <xref rid="B179-marinedrugs-17-00314" ref-type="bibr" class="xref">179</xref>]
    </td>
   </tr>
   <tr class="tr">
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">Acid hydrolysis</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">
     <italic class="italic">Eucheuma cottonii</italic>
    </td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">DP: κ-, 6–20</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1" class="td">[
     <xref rid="B180-marinedrugs-17-00314" ref-type="bibr" class="xref">180</xref>]
