       <mi class="mi">T</mi>
       <mo class="mo">−</mo>
       <mn class="mn">0.00002</mn>
       <mi class="mi">D</mi>
       <msup class="msup">
        <mi class="mi">T</mi>
        <mn class="mn">2</mn>
       </msup>
       <mo class="mo">+</mo>
       <mn class="mn">0.0000001</mn>
       <mi class="mi">D</mi>
       <msup class="msup">
        <mi class="mi">T</mi>
        <mn class="mn">3</mn>
       </msup>
      </mrow>
     </mrow>
    </math>
