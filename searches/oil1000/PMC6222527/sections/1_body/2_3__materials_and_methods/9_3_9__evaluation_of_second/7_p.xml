<p class="p">Experiment 2:
 <disp-formula id="FD5-molecules-23-02052" class="disp-formula">
  <label class="label">(5)</label>
  <math id="mm5" class="math">
   <mrow class="mrow">
    <mrow class="mrow">
     <mi class="mi">y</mi>
     <mo class="mo">=</mo>
     <mi class="mi">c</mi>
     <mo class="mo">+</mo>
     <mfrac class="mfrac">
      <mrow class="mrow">
       <mi class="mi">d</mi>
       <mo class="mo">−</mo>
       <mi class="mi">c</mi>
      </mrow>
      <mrow class="mrow">
       <mrow class="mrow">
        <mo class="mo">(</mo>
        <mrow class="mrow">
         <mn class="mn">1</mn>
         <mo class="mo">+</mo>
         <mi class="mi">e</mi>
         <mi class="mi">x</mi>
         <mi class="mi">p</mi>
         <mrow class="mrow">
          <mo stretchy="false" class="mo">(</mo>
          <mrow class="mrow">
           <mi class="mi">b</mi>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mrow class="mrow">
             <mi class="mi">l</mi>
