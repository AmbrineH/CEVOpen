<?xml version="1.0" encoding="UTF-8"?>
<glossary class="glossary">
 <title class="title">Abbreviations</title>
 <def-list id="DL1" class="def-list">
  <def-item class="def-item">
   <term class="term">cLRG</term>
   <def class="def">
    <p class="p">clean-leaf rolling galls, where cLRG stands for clean leaf galls</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">Ctrl</term>
   <def class="def">
    <p class="p">control individual</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">LRGwT</term>
   <def class="def">
    <p class="p">leaf rolling galls without thrips, where “wT” stands for “with thrips”</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">NGL</term>
   <def class="def">
    <p class="p">non-galled leaves</p>
   </def>
  </def-item>
 </def-list>
</glossary>
