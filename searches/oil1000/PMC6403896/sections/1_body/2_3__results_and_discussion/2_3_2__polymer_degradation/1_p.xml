      <mn class="mn">39.618</mn>
      <mo class="mo">×</mo>
      <msup class="msup">
       <mi mathvariant="normal" class="mi">e</mi>
       <mrow class="mrow">
        <mo class="mo">−</mo>
        <mn class="mn">0.003</mn>
        <msub class="msub">
         <mi class="mi">t</mi>
