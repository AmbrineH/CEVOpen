      <mi class="mi">i</mi>
      <mi class="mi">g</mi>
      <mi class="mi">h</mi>
      <mi class="mi">t</mi>
      <mspace width="0.25em" class="mspace"/>
      <mrow class="mrow">
       <mo stretchy="true" class="mo">(</mo>
       <mo class="mo">%</mo>
       <mo stretchy="true" class="mo">)</mo>
      </mrow>
      <mo class="mo">×</mo>
      <mn class="mn">100</mn>
     </mrow>
     <mrow class="mrow">
      <mi class="mi">D</mi>
      <mi class="mi">r</mi>
      <mi class="mi">y</mi>
      <mspace width="0.25em" class="mspace"/>
