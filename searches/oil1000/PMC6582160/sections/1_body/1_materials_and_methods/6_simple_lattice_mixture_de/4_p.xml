      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">3</mn>
     </mrow>
    </msub>
    <mo linebreak="goodbreak" class="mo">+</mo>
    <mspace width="0.25em" class="mspace"/>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">β</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">12</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">1</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">2</mn>
     </mrow>
    </msub>
    <mo linebreak="goodbreak" class="mo">+</mo>
    <mspace width="0.25em" class="mspace"/>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">β</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">13</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">1</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">3</mn>
     </mrow>
    </msub>
    <mo linebreak="goodbreak" class="mo">+</mo>
    <mspace width="0.25em" class="mspace"/>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">β</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">23</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">2</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">3</mn>
     </mrow>
    </msub>
   </mrow>
  </math>
 </disp-formula>
</p>
