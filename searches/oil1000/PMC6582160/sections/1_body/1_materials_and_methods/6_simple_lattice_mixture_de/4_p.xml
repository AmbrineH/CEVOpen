      <mi class="mi">j</mi>
     </mrow>
     <mo stretchy="true" class="mo">)</mo>
    </mrow>
    <mo linebreak="goodbreak" class="mo">=</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">β</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">1</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">1</mn>
     </mrow>
    </msub>
    <mo linebreak="goodbreak" class="mo">+</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">β</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">2</mn>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">X</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">2</mn>
      <mspace width="0.25em" class="mspace"/>
     </mrow>
    </msub>
    <mo linebreak="goodbreak" class="mo">+</mo>
    <mspace width="0.25em" class="mspace"/>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">β</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">3</mn>
