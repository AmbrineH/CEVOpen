<?xml version="1.0" encoding="UTF-8"?>
<table-wrap id="T1" orientation="portrait" position="float" class="table-wrap">
 <label class="label">Table 1.</label>
 <caption class="caption">
  <title class="title">Methanogenesis pathways</title>
 </caption>
 <table frame="hsides" rules="groups" class="table">
  <thead class="thead">
   <tr class="tr">
    <th valign="top" align="center" rowspan="1" colspan="1" class="th">Pathways</th>
    <th valign="top" align="center" rowspan="1" colspan="1" class="th">Reactions</th>
    <th valign="top" align="center" rowspan="1" colspan="1" class="th">References</th>
   </tr>
  </thead>
  <tbody class="tbody">
   <tr class="tr">
    <td valign="top" align="left" rowspan="1" colspan="1" class="td">Hydrogenotrophic pathway</td>
    <td valign="top" align="left" rowspan="1" colspan="1" class="td">CO
     <sub class="sub">2</sub> + 4H
     <sub class="sub">2</sub> → CH
     <sub class="sub">4</sub> + 2H
     <sub class="sub">2</sub>O
    </td>
    <td valign="top" align="center" rowspan="1" colspan="1" class="td">[
     <xref rid="B57" ref-type="bibr" class="xref">57</xref>,
     <xref rid="B58" ref-type="bibr" class="xref">58</xref>,
     <xref rid="B87" ref-type="bibr" class="xref">87</xref>,
     <xref rid="B89" ref-type="bibr" class="xref">89</xref>]
    </td>
   </tr>
   <tr class="tr">
    <td valign="top" align="left" rowspan="1" colspan="1" class="td">Methylotrophic pathway</td>
    <td valign="top" align="left" rowspan="1" colspan="1" class="td">CH
     <sub class="sub">3</sub>OH + H
     <sub class="sub">2</sub> → CH
     <sub class="sub">4</sub> + H
     <sub class="sub">2</sub>O
     <break class="break"/>4CH
     <sub class="sub">3</sub>OH → 3CH
     <sub class="sub">4</sub> + CO
     <sub class="sub">2</sub> + 2H
     <sub class="sub">2</sub>O
     <break class="break"/>CH
     <sub class="sub">3</sub>NH
     <sub class="sub">2</sub> + H
     <sub class="sub">2</sub> → CH
     <sub class="sub">4</sub> + NH
     <sub class="sub">3</sub>
    </td>
    <td valign="top" align="center" rowspan="1" colspan="1" class="td">[
     <xref rid="B98" ref-type="bibr" class="xref">98</xref>,
     <xref rid="B121" ref-type="bibr" class="xref">121</xref>]
    </td>
   </tr>
  </tbody>
 </table>
 <table-wrap-foot class="table-wrap-foot">
  <fn id="TN1" class="fn">
   <p class="p">CO
    <sub class="sub">2</sub>, carbon dioxide; H
    <sub class="sub">2</sub>, hydrogen; CH
    <sub class="sub">4</sub>, methane; H
    <sub class="sub">2</sub>O, water; CH
    <sub class="sub">3</sub>OH, methanol; CH
    <sub class="sub">3</sub>NH
    <sub class="sub">2</sub>, methylamine.
   </p>
  </fn>
 </table-wrap-foot>
</table-wrap>
