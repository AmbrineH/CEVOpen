    <tr class="tr">
     <th rowspan="1" colspan="1" class="th">Plants</th>
     <th rowspan="1" colspan="1" class="th">Compounds</th>
    </tr>
   </thead>
   <tbody class="tbody">
    <tr class="tr">
     <td rowspan="1" colspan="1" class="td">
      <italic class="italic">Bunium persicum</italic> essential oil
     </td>
     <td rowspan="1" colspan="1" class="td">β-pinene (11.72%), 
      <italic class="italic">p</italic>-cymene (15.47%), γ-terpinene (18.32%), cumin aldehyde (38.4%), 
      <italic class="italic">p</italic>-mentha-1,3-dien-7-al (5.37%), and 
      <italic class="italic">p</italic>-mentha-1,4-dien-7-al (2.86%)
     </td>
    </tr>
    <tr class="tr">
     <td rowspan="1" colspan="1" class="td">
      <italic class="italic">Eucalyptus globulus</italic> essential oil
     </td>
     <td rowspan="1" colspan="1" class="td">Limonene (9.4%) and 1,8-cineole (70.3%)</td>
    </tr>
    <tr class="tr">
     <td rowspan="1" colspan="1" class="td">Rose water</td>
     <td rowspan="1" colspan="1" class="td">Linalool (6.6%), terpineol (3.3%), carvone (0.31%), citronellol (6.85%), 
      <italic class="italic">trans</italic>-geraniol (7.11%), phenylethanol (66.84%), eugenol (4.53%), cytronellol, hydroxyl (1.15%), and geranic acid (1.2%)
     </td>
    </tr>
