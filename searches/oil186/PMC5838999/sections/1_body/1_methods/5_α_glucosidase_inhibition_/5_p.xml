    </msub>
    <msub class="msub">
     <mi mathvariant="normal" class="mi">V</mi>
     <mi class="mi">max</mi>
    </msub>
    <mspace width="0.25em" class="mspace"/>
    <mfenced close="]" open="[" class="mfenced">
